{
    "properties":{
        "libDir":"./lib",
        "pluginsDir":"./plugins"
    },

    "controllers":{
        "cli.clamshell.impl.CmdController":{
            "enabled":"true",
            "inputPattern":"\\s*(exit|help|sysinfo|time)\\b.*"
        },
        "cli.clamshell.jmx.JmxController":{
            "enabled":"true",
            "inputPattern":"\\s(ps)\\b.*"
        },
        "cli.clamshell.impl.EchoController":{
            "enabled":"false",
            "inputPattern":".*"
        }
    }
}